function showpopup(e,t){$("body").addClass("td-body_popup-opened"),$(e).addClass("td-popup_opened"),$(e).find(".td-popup-window").html(t),$(e).fadeIn("fast"),$(document).keyup(keyUpFunc),delete window.pspopup_isChanged}function closepopup(){var e;void 0!==window.pspopup_isChanged&&"y"==window.pspopup_isChanged&&(console.log("closed without saving changes"),!confirm("RU"==lang?"Последние изменения не сохранены. Закрыть панель?":"You have unsaved changes. Are you sure you want to close this panel?"))||($("body").removeClass("td-body_popup-opened"),"notdelete"!=(e=$(".td-popup_opened")).data("popup-content")&&e.find(".td-popup-window").html(""),e.fadeOut(),$(document).unbind("keyup",keyUpFunc),delete window.pspopup_isChanged)}function init_popup(){try{$(".input-file input[type=file]").styler({filePlaceholder:"Upload image"}),$(".checkbox-btn").styler()}catch(e){console.log(e)}$(".td-item-group").on("mouseenter focus","input",function(e){$(e.delegateTarget).addClass("td-item-group_active")}).on("mouseleave blur","input",function(e){"mouseleave"==e.type&&document.activeElement==this||$(e.delegateTarget).removeClass("td-item-group_active")});var o,e,t,p=$(".td-popup-menu__wrapper");0<p.length&&(o=$("#td_popup_menu__line"),e=(t=p.find(".td-popup-menu__item_active")).offset().left-p.offset().left,t=t.outerWidth(),o.css({left:e,width:t}),$(".td-popup-menu__item").click(function(){var e=$(this),t=e.offset().left-p.offset().left;o.stop().animate({width:e.outerWidth(),left:t},400)}))}function keyUpFunc(e){27==e.keyCode&&closepopup()}function showbyid_and_hidebutton(e,t){$(e).css("display","block"),$(t).css("display","none")}function switchtab(e){$(".td-tab-content").find(".td-tab-pane").removeClass("td-tab-pane_active"),$(e).addClass("td-tab-pane_active")}function getCookie(e){e=("; "+document.cookie).split("; "+e+"=");if(2==e.length)return e.pop().split(";").shift()}
